{
    "name": "knit-pay/omnipay-paypal",
    "type": "library",
    "description": "PayPal gateway for Omnipay payment processing library",
    "keywords": [
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment",
        "paypal",
        "purchase"
    ],
    "homepage": "https://github.com/thephpleague/omnipay-paypal",
    "license": "MIT",
    "authors": [
        {
            "name": "Knit Pay",
            "homepage": "https://github.com/knit-pay/omnipay-paypal"
        }
    ],
    "autoload": {
        "psr-4": { "Omnipay\\PayPal\\" : "src/" }
    },
    "require": {
        "php": "^7.2|^8.0",
        "omnipay/common": "^3"
    },
    "require-dev": {
        "omnipay/tests": "^4.1.2",
        "squizlabs/php_codesniffer": "^3"
    },
    "replace" : {
        "omnipay/paypal": "^3.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 src/",
        "fix-style": "phpcbf -p --standard=PSR2 src/"
    },
    "prefer-stable": true
}
